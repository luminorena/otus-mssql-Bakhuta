  <DataSets>
    <DataSet Name="Prices">
      <Query>
        <DataSourceName>Prices</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@monthName">
            <Value>=Parameters!monthName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>use VetShop

select Quantity, CostPerUnit, SaleDate, 
CustomerName, DeliveryAddress from Sales.Orders o
join Sales.Customers c
on o.CustomerId = c.CustomerId
where month(SaleDate) = @monthName</CommandText>
      </Query>
      <Fields>
        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="SaleDate">
          <DataField>SaleDate</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CostPerUnit">
          <DataField>CostPerUnit</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Price">
          <Value>=Fields!Quantity.Value*Fields!CostPerUnit.Value</Value>
        </Field>
        <Field Name="DeliveryAddress">
          <DataField>DeliveryAddress</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
