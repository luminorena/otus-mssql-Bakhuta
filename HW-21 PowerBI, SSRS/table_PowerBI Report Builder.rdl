  <DataSets>
    <DataSet Name="Sales">
      <Query>
        <DataSourceName>VetShop_sales</DataSourceName>
        <CommandText>use VetShop

select Quantity, CostPerUnit, Quantity*CostPerUnit as Price,
SaleDate from dbo.CustomerPayments c
join dbo.Orders o
on c.OrderId = o.OrderID
</CommandText>
      </Query>
      <Fields>
        <Field Name="Quantity">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
        <Field Name="CostPerUnit">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>CostPerUnit</DataField>
        </Field>
        <Field Name="Price">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Price</DataField>
        </Field>
        <Field Name="SaleDate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>SaleDate</DataField>
        </Field>
      </Fields>
    </DataSet>
